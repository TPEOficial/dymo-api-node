{
  "name": "dymo-api",
  "version": "1.0.61",
  "description": "Flow system for Dymo API.",
  "main": "dist/cjs/dymo-api.js",
  "module": "dist/esm/dymo-api.js",
  "types": "dist/types/dymo-api.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/dymo-api.js",
      "import": "./dist/esm/dymo-api.js",
      "default": "./dist/esm/dymo-api.js",
      "types": "./dist/types/dymo-api.d.ts"
    },
    "./*": {
      "types": "./build/types/*.d.ts",
      "require": "./build/cjs/*.js",
      "import": "./build/esm/*.js",
      "default": "./build/esm/*.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "start": "ts-node src/dymo-api.ts",
    "prepublishOnly": "npm run build",
    "test": "node --max-old-space-size=4096 test/index.js"
  },
  "repository": {
    "url": "https://github.com/TPEOficial/dymo-api-node"
  },
  "keywords": [
    "Dymo",
    "Dymo API",
    "TPEOficial",
    "Ciphera"
  ],
  "author": "TPEOficial LLC",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TPEOficial/dymo-api-node/issues"
  },
  "homepage": "https://dymo.tpeoficial.com",
  "dependencies": {
    "@react-email/render": "1.0.1",
    "@types/react": "^18.3.5",
    "axios": "^1.6.8",
    "path": "^0.12.7",
    "rimraf": "^6.0.1",
    "tw-to-css": "0.0.12"
  },
  "contributors": [
    "TPEOficial (https://github.com/TPEOficial)",
    "FJRG2007 (https://github.com/FJRG2007)"
  ],
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^22.9.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}